<div class="logo_type_not_home">
    <h1><%=@post.date%></h1> 
</div>

  <div class="wrapper">

<h3><%=@post.headline%></h3>

<%if @post.icon%>
  <%= image_tag @post.icon, class: 'title_icon', id: 'headline_icon' %>
<%end%>

<p><%= simple_format(@post.content) %></p>

<%@post.post_images.each do |img|%>
  <%= image_tag img.image_url %>
<%end%>

<!-- needs an edit button for me and a comment(maybe just tags?) options for the public -->

</div>